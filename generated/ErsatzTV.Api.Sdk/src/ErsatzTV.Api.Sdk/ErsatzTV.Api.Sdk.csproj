    <Copyright>No Copyright</Copyright>
    <RootNamespace>ErsatzTV.Api.Sdk</RootNamespace>
    <Version>1.0.0</Version>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\ErsatzTV.Api.Sdk.xml</DocumentationFile>
